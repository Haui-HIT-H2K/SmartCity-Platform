# Example environment variables for consumer-worker
# Copy to .env and modify as needed

# RabbitMQ Edge Node 1
RABBITMQ_HOST_1=localhost
RABBITMQ_PORT_1=5672
RABBITMQ_QUEUE_1=sensor_queue_1

# RabbitMQ Edge Node 2
RABBITMQ_HOST_2=localhost
RABBITMQ_PORT_2=5673
RABBITMQ_QUEUE_2=sensor_queue_2

# RabbitMQ Credentials
RABBITMQ_USER=edge_user
RABBITMQ_PASS=edge_pass
RABBITMQ_VHOST=/

# Which queue to consume (1 or 2)
CONSUME_QUEUE=1

# ML Service
ML_SERVICE_URL=http://localhost:8000

# Consumer Settings
PREFETCH_COUNT=10
MAX_RETRIES=3
RETRY_DELAY=2
CONNECTION_TIMEOUT=10
HTTP_TIMEOUT=5

# Logging
LOG_LEVEL=INFO
